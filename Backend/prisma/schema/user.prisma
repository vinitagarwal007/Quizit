enum authProvider{
    Email
    Google
}

enum role{
    Admin
    Student
    Teacher
}
model user {
    uid  Int @id @default(autoincrement())
    name String
    username String? @unique
    password String?
    salt String?
    rollno Int? @unique
    provider authProvider @default(Email)
    providerToken String?
    role role @default(Student)

    testManager testManager[]
    submission submission[]
    test test[]
}
