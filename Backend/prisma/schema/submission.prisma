model submission{
    sid Int @id @default(autoincrement())
    tid Int
    uid Int
    qid Int @unique
    marks_obtained Int
    is_ai Boolean
    submitted_at DateTime

    test test @relation(fields: [tid], references: [tid])
    user user @relation(fields: [uid], references: [uid])
    question questionBank @relation(fields: [qid], references: [qid])

    fileManager fileManager[]
}