model test{
    tid Int @id @default(autoincrement())
    name String
    semester Int
    subjectID Int
    violation_count Int @default(3)
    question_count Int @default(0)
    instructions String?
    start DateTime
    end DateTime
    suffle Boolean @default(true)
    proctoring Boolean @default(true)
    navigation Boolean @default(false)
    report_published Boolean @default(false)
    created_by Int

    subject subject @relation(fields: [subjectID], references: [sid])
    user user @relation(fields: [created_by], references: [uid])
    testManager testManager[]
    submission submission[]
    questionBank questionBank[]
}